<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE 
suppressions PUBLIC "-//Checkstyle//DTD SuppressionXpathFilter Experimental Configuration 1.2//EN"
"https://checkstyle.org/dtds/suppressions_1_2_xpath_experimental.dtd">
<suppressions>
  <suppress-xpath files="ScannerRegex.java"
                  checks="InnerAssignmentCheck"
                  query="/ENUM_DEF[./IDENT[@text='ScannerRegex']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='main']]/SLIST/LITERAL_DO/SLIST/LITERAL_FOR/FOR_CONDITION/EXPR/NOT_EQUAL/ASSIGN[./IDENT[@text='str']]" />
  <suppress-xpath files="SqlTypeRegex.java"
                  checks="MagicNumberCheck"
                  query="/ENUM_DEF[./IDENT[@text='SqlTypeRegex']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='main']]/SLIST/VARIABLE_DEF[./IDENT[@text='precision']]/ASSIGN/EXPR/METHOD_CALL[./IDENT[@text='getColumnDatatypeComponent']]/ELIST/EXPR/NUM_INT[@text='3']" />
</suppressions>
