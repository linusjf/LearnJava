<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE 
suppressions PUBLIC "-//Checkstyle//DTD SuppressionXpathFilter Experimental Configuration 1.2//EN"
"https://checkstyle.org/dtds/suppressions_1_2_xpath_experimental.dtd">
<suppressions>
  <suppress-xpath files="ClassCreationDemoMaker.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='ClassCreationDemoMaker']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='dump']]/SLIST/SLIST/EXPR/METHOD_CALL/ELIST/EXPR/NUM_INT[@text='3']" />
  <suppress-xpath files="MakeHelloWorld.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='MakeHelloWorld']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='addMainMethod']]/SLIST/EXPR/METHOD_CALL/ELIST/EXPR/NUM_INT[@text='3']" />
  <suppress-xpath files="MakeHelloWorld.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='MakeHelloWorld']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='addMainMethod']]/SLIST/EXPR/METHOD_CALL/ELIST/EXPR/NUM_INT[@text='3']" />
</suppressions>
