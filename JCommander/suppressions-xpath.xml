<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE 
suppressions PUBLIC "-//Checkstyle//DTD SuppressionXpathFilter Experimental Configuration 1.2//EN"
"https://checkstyle.org/dtds/suppressions_1_2_xpath_experimental.dtd">
<suppressions>
  <suppress-xpath files="DelegateCommand.java"
                  checks="OuterTypeNumberCheck"
                  query="/PACKAGE_DEF[./IDENT[@text='command']]" />
  <suppress-xpath files="DelegateCommand.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='DelegateCommand']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='main']]/SLIST/EXPR/METHOD_CALL/ELIST/EXPR/NUM_INT[@text='1234']" />
  <suppress-xpath files="TooledCommand.java"
                  checks="OuterTypeNumberCheck"
                  query="/PACKAGE_DEF[./IDENT[@text='command']]" />
  <suppress-xpath files="ArgsCommand.java"
                  checks="OuterTypeNumberCheck"
                  query="/PACKAGE_DEF[./IDENT[@text='command']]" />
</suppressions>
