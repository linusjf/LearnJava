<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE 
suppressions PUBLIC "-//Checkstyle//DTD SuppressionXpathFilter Experimental Configuration 1.2//EN"
"https://checkstyle.org/dtds/suppressions_1_2_xpath_experimental.dtd">
<suppressions>
<suppress-xpath
       files="Pizza.java"
       checks="AbstractClassNameCheck"
       query="/CLASS_DEF[./IDENT[@text='Pizza']]"/>
<suppress-xpath
       files="Pizza.java"
       checks="AbstractClassNameCheck"
       query="/CLASS_DEF[./IDENT[@text='Pizza']]/OBJBLOCK/CLASS_DEF[./IDENT[@text='Builder']]"/>
  <suppress-xpath files="Calzone.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='Calzone']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='toString']]/SLIST/VARIABLE_DEF[./IDENT[@text='sb']]/ASSIGN/EXPR/LITERAL_NEW[./IDENT[@text='StringBuilder']]/ELIST/EXPR/NUM_INT[@text='30']" />
  <suppress-xpath files="NutritionFacts.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='NutritionFacts']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='toString']]/SLIST/VARIABLE_DEF[./IDENT[@text='sb']]/ASSIGN/EXPR/LITERAL_NEW[./IDENT[@text='StringBuilder']]/ELIST/EXPR/NUM_INT[@text='100']" />
  <suppress-xpath files="NyPizza.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='NyPizza']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='toString']]/SLIST/VARIABLE_DEF[./IDENT[@text='sb']]/ASSIGN/EXPR/LITERAL_NEW[./IDENT[@text='StringBuilder']]/ELIST/EXPR/NUM_INT[@text='36']" />
  <suppress-xpath files="Car.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='Car']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='toString']]/SLIST/VARIABLE_DEF[./IDENT[@text='sb']]/ASSIGN/EXPR/LITERAL_NEW[./IDENT[@text='StringBuilder']]/ELIST/EXPR/NUM_INT[@text='150']" />
  <suppress-xpath files="Form.java"
                  checks="MagicNumberCheck"
                  query="/CLASS_DEF[./IDENT[@text='Form']]/OBJBLOCK/METHOD_DEF[./IDENT[@text='toString']]/SLIST/VARIABLE_DEF[./IDENT[@text='sb']]/ASSIGN/EXPR/LITERAL_NEW[./IDENT[@text='StringBuilder']]/ELIST/EXPR/NUM_INT[@text='100']" />
</suppressions>
