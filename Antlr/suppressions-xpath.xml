                  checks="PackageNameCheck"
                  query="/COMPILATION_UNIT/PACKAGE_DEF/IDENT[@text='java6']" />
  <suppress-xpath files="InsertSerialID.java"
                  checks="PackageNameCheck"
                  query="/COMPILATION_UNIT/PACKAGE_DEF/IDENT[@text='java6']" />
  <suppress-xpath files="JSON2XML.java"
