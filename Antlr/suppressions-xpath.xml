  <suppress-xpath files="Demo.java"
                  checks="MethodNameCheck"
                  query="/COMPILATION_UNIT/CLASS_DEF[./IDENT[@text='Demo']]/OBJBLOCK/METHOD_DEF/IDENT[@text='g']" />
  <suppress-xpath files="Demo.java"
                  checks="MethodNameCheck"
                  query="/COMPILATION_UNIT/CLASS_DEF[./IDENT[@text='Demo']]/OBJBLOCK/METHOD_DEF/IDENT[@text='h']" />
