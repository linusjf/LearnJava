  <suppress-xpath files="DisplayParseTree.java"
                  checks="PackageNameCheck"
                  query="/COMPILATION_UNIT/PACKAGE_DEF/IDENT[@text='xmlv2']" />
  <suppress-xpath files="DisplayTokens.java"
