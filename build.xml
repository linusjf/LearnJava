  <target name="removecpjars"
          if="cpfiles">
    <for list="${cpfiles}"
         delimiter=","
         param="cpfile"
         parallel="false">
      <sequential>
        <echo message="@{cpfile}" />
        <loadfile srcFile="@{cpfile}"
                  property="@{cpfile}" />
        <echo message="${@{cpfile}}" />
        <for list="${@{cpfile}}"
             delimiter=":"
             param="jar"
             parallel="false">
          <sequential>
            <echo message="@{jar}" />
            <delete file="@{jar}" />
          </sequential>
        </for>
      </sequential>
    </for>
  </target>
  <target name="removeduplicates"
          if="file">
    <loadfile srcFile="${file}"
              property="input">
      <filterchain>
        <tokenfilter>
          <filetokenizer />
          <replacestring from=":"
                         to="${line.separator}" />
          <replaceregex pattern="\Z"
                        replace="${line.separator}" />
        </tokenfilter>
        <ignoreblank />
        <trim />
        <sortfilter />
        <uniqfilter />
        <tokenfilter>
          <filetokenizer />
          <replacestring from="${line.separator}"
                         to=":" />
          <replaceregex pattern="^:|:$"
                        replace="" />
        </tokenfilter>
      </filterchain>
    </loadfile>
    <echo file="${file}"
          message="${input}" />
  </target>
  <target name="uniquify">
    <for param="theFile"
         parallel="false">
      <path>
        <fileset dir="${LearnJava.basedir}"
                 includes="*.cp" />
      </path>
      <sequential>
        <echo message="@{theFile}" />
        <antcall target="removeduplicates">
          <param name="file"
                 value="@{theFile}" />
        </antcall>
      </sequential>
    </for>
  </target>
